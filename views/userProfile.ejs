<%- include('./partials/header') %>
<%- include('./partials/nav') %>

<div class="container portfolio">
    <div class="row">
        <div class="col-md-12">
            <div class="heading">
                <img src="https://image.ibb.co/cbCMvA/logo.png" />
            </div>
        </div>
    </div>
    <div class="bio-info">
        <div class="row">
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-12">
                        <div class="bio-image">
                            <img src="<%= u.profilePic %>" alt="Your google image" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="bio-content">
                    <h3><%= u.name %></h3>
                    <h6>Email: <%= u.email %></h6>
                </div>
            </div>
        </div>
    </div>
</div>

<%- include('./partials/footer') %>